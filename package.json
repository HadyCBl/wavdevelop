{
  "name": "microsystemplus",
  "version": "2.0.1",
  "description": "Microsystem Plus - Admin Dashboard Template",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --config webpack.config.js",
    "dev": "webpack --config webpack.config.js --env mode=development --watch",
    "dev:another": "webpack --config webpack.config.js --env target=another --env mode=development --watch",
    "dev:caja": "webpack --config webpack.config.js --env target=caja --env mode=development --watch",
    "build": "webpack --config webpack.config.js --env mode=production",
    "build:all": "webpack --config webpack.config.js --env mode=production",
    "build:another": "webpack --config webpack.config.js --env target=another --env mode=production",
    "build:caja": "webpack --config webpack.config.js --env target=caja --env mode=production",
    "clean": "rm -rf public/assets/dist .webpack-cache",
    "vite:dev": "vite --config vite.config.js",
    "vite:build": "vite build --config vite.config.js --mode production",
    "vite:preview": "vite preview --config vite.config.js",
    "vite:clean": "rm -rf public/assets/vite-dist",
    "sort": "npx prettier --write src",
    "release": "release-please --release-type=php --package-name=microsystemplus",
    "test": "./vendor/bin/phpunit"
  },
  "keywords": [],
  "author": {
    "name": "Sotecpro",
    "email": "dd@sotecpro.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.0.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-prismjs": "^2.1.0",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "daisyui": "^5.0.35",
    "file-loader": "^6.2.0",
    "glob": "^11.0.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.39",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "release-please": "^17.0.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "vite": "^7.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-manifest": "^0.0.1",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-manifest-plugin": "^5.0.1"
  },
  "dependencies": {
    "@alpinejs/persist": "^3.14.1",
    "ably": "^2.4.0",
    "alpinejs": "^3.14.1",
    "apexcharts": "^3.51.0",
    "cleave-zen": "^0.0.17",
    "datatables.net-dt": "^2.3.4",
    "datatables.net-plugins": "^2.3.2",
    "dropzone": "^6.0.0-beta.2",
    "flatpickr": "^4.6.13",
    "jquery": "^3.7.1",
    "select2": "^4.1.0-rc.0",
    "sweetalert2": "^11.26.3",
    "swiper": "^11.1.14",
    "tom-select": "^2.4.3"
  }
}
